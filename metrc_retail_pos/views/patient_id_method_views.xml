<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="patient_id_method_views_tree" model="ir.ui.view">
        <field name="name">patient.id.method.view.tree</field>
        <field name="model">patient.id.method</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="action_open_patient_id_method" model="ir.actions.act_window">
        <field name="name">Patient ID Method</field>
        <field name="res_model">patient.id.method</field>
        <field name="view_mode">tree</field>
        <field name="domain">[]</field>
        <field name="context">{}</field>
    </record>
    <menuitem id="menu_conf_action_open_patient_id_method"
        action="action_open_patient_id_method"
        name="Patient ID Methods"
        parent="metrc_base.submenu_metrc_data"
        groups="metrc_base.group_metrc_manager"/>
</odoo>