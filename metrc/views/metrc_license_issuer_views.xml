<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="metrc_license_issuer_form" model="ir.ui.view">
		<field name="name">Metrc License Issuer View Tree</field>
		<field name="model">metrc.license.issuer</field>
		<field name="arch" type="xml">
			<tree string="Metrc License Issuers" editable="bottom">
				<field name="name"/>
			</tree>
		</field>
	</record>
	<record id="action_view_metrc_license_issuer" model="ir.actions.act_window">
		<field name="name">Metrc License Issuer</field>
		<field name="res_model">metrc.license.issuer</field>
		<field name="view_mode">tree</field>
		<field name="context">{}</field>
		<field name="domain">[]</field>
	</record>
	<menuitem id="menu_conf_metrc_license_issuer" action="action_view_metrc_license_issuer" name="License Issuer" parent="metrc_base.submenu_metrc_data"/>
</odoo>