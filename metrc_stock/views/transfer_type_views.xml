<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id="metrc_transfer_type_view_tree" model="ir.ui.view">
		<field name="name">metrc.transfer.type.view.tree</field>
		<field name="model">metrc.transfer.type</field>
		<field name="arch" type="xml">
			<tree create="0" edit="0">
				<field name="name"/>
				<field name="licensed_shipments"/>
				<field name="external_incoming_shipments"/>
				<field name="external_outgoing_shipments"/>
				<field name="gross_weight_required"/>
			</tree>
		</field>
	</record>
	<record id="action_open_metrc_transfer_type" model="ir.actions.act_window">
		<field name="name">Metrc Transfer Types</field>
		<field name="res_model">metrc.transfer.type</field>
		<field name="view_mode">tree</field>
		<field name="domain">[]</field>
		<field name="context">{}</field>
	</record>
	<menuitem id="menu_conf_metrc_transfer_types" action="action_open_metrc_transfer_type"
			  name="Transfer Types" parent="metrc_base.submenu_metrc_data"/>
</odoo>